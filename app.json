{
  "expo": {
    "name": "wikiluke",
    "slug": "wikiluke",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lukemackenzie.wikiluke",
      "buildNumber": "1.0.0"
    },
    "description": "A handy app to quickly add data to my personal wiki page."
  }
}
